{
  "buildCommand": "pnpm --filter ./packages/fhevm-sdk build && pnpm --filter ./packages/frontend-react build",
  "outputDirectory": "packages/frontend-react/dist",
  "installCommand": "pnpm install --shamefully-hoist --no-frozen-lockfile",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
